# Copy this file to $HOME/.bash_aliases

# git
alias ga='git add'
alias gco='git checkout'
alias gcl='git clone'
alias gcm='git commit'
alias gd='git diff'
alias gi='git init'
alias gl='git log'
alias gme='git merge'
alias gmv='git mv'
alias gpull='git pull'
alias gpush='git push'
alias grm='git rm'
alias gst='git status'



# general
alias rs='reset'
alias shutdown='sudo shutdown -h now'
alias reboot='sudo reboot'
alias ..='cd ..'
alias ...='cd ../../'
alias ~='cd ~'
